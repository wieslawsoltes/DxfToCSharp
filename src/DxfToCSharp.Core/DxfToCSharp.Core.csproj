<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <PackageId>DxfToCSharp.Core</PackageId>
    <Description>A library for generating C# code from DXF documents using netDxf</Description>
    <PackageTags>dxf;cad;code-generation;netdxf;csharp</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="netDxf.netstandard" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Workspaces" />
  </ItemGroup>

</Project>
